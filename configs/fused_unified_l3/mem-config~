[ General]
Frequency = 3000

;16KB SI L1 Cache
[CacheGeometry geo-si-l1]
Sets = 64
Assoc = 1
BlockSize = 64
Latency = 1

; 128KB SI L2 Cache (1 for entire GPU)
[CacheGeometry geo-si-l2]
Sets = 128
Assoc = 16
BlockSize = 64
Latency = 10

; 32KB x86 L1 data cache
[CacheGeometry geo-x86-l1-data]
Sets = 64
Assoc = 8
BlockSize = 64
Latency = 1

;32KB x86 L1 instruction cache
[ CacheGeometry geo-x86-l1-inst]
Sets = 64
Assoc = 8
BlockSize = 64
Latency = 1

; 256KB x86 L2 cache (1 per core)
[CacheGeometry geo-x86-l2]
Sets = 512
Assoc = 8
BlockSize = 64
Latency = 10

; 8MB SI L3 Cache ( Share between CPU and GPU)
[CacheGeometry geo-shared-l3]
Sets = 8192
Assoc = 16
BlockSize = 64
Latency = 20

; 128 MB L4 cache ( 1 for entire system)
[CacheGeometry geo-shared-l4]
Sets =  8192
Assoc = 256
BlockSize = 64
Latency = 100


;--------Main Memory--------
[Module mod-shared-mm]
Type = MainMemory
BlockSize = 64
Latency = 1
HighNetwork = net-l4-mm

;------- Shared L4 Cache-------
[Module mod-shared-l4]
Type = Cache
Geometry = geo-shared-l4
HighNetwork = net-l3-l4
LowNetwork = net-l4-mm
LowModules = mod-shared-mm

;--------Shared L3 Cache-----------
[Module mod-shared-l3]
Type = Cache
Geometry = geo-shared-l3
HighNetwork = net-l2-l3
LowNetwork = net-l3-l4
LowModules = mod-shared-l4

